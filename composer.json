{
    "name": "languara/plugin",
    "keywords": ["Languara", "laravel"],
    "homepage": "http://languara.com",
    "description": "Languara integration plugin for Laravel 4. Easily synchronize your content with Languara.",
    "authors": [
        {
            "name": "Aleksandar Tufekciev",
            "email": "aleksandar@languara.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*"
    },
    "autoload": {
        "psr-0": {
            "Languara\\Plugin": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
